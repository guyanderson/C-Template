<h1>Add a CD to your Artist.</h1>
<h3>@Model["Artist"].GetName()</h3>
<ol>
@foreach (var cd in Model["CDs"])
{
  <li>@cd.GetName()</li>
}
</ol>
<form action='/AddCd/@Model["Artist"].GetId()' method='post'>
  <label for="CD-name">New CD name</label>
  <input id="CD-name" name="CD-name" type="text">
  <button type='submit'>Add CD</button>
</form>
